
@{
    ViewBag.Title = "Negocios";
}
<script>
    var fcallback_buscador = function ($arg) {
        console.log($arg);
        $("#divResultados").empty();
        initListarResultados($("#divResultados"), $arg);
    };

    $(document).ready(function () {
        $("#divBuscador").empty();
        initBuscadorNegocio($("#divBuscador"), fcallback_buscador);

        initLogin($("#divLogin"));
    });
</script>
<h2>@ViewBag.Title</h2>

<div class="well text-primary" id="divBuscador">Buscador de negocios</div>
<div class="row">
    <div class="col-md-8">
        <div class="row">
            <div class="col-md-12">
                <div class="well text-primary" id="divResultados">
                </div>
            </div>
            <div class="col-md-12">
                <div class="well text-primary" id="divMapa">
                </div>
            </div>
        </div>
    </div>
    <div class="col-md-4">
        <div class="row">
            <div class="col-md-12">
                <div class="well text-primary" id="divLogin">
                    @Html.ActionLink("Iniciar sesión", "Login")
                </div>
            </div>
            <div class="col-md-12">
                <div class="well text-primary" id="divAbout">
                    Acerca de...
                </div>
            </div>
        </div>
    </div>
</div>