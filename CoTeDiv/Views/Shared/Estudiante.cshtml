@model CoTeDiv.Models.ResponseModel
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - CoTeDiv</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
</head>
<body>
    <nav class="navbar navbar-inverse navbar-fixed-top">
        <div class="container-fluid">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Cotediv", "Inicio", "Estudiante", new {
               hash = (Session["Usuario"] as SernaSistemas.Core.Models.LoginModel).Hash
           }, new {
               @class = "navbar-brand"
           })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>
                        @Html.ActionLink(linkText: "Publicar", actionName: "Conceptuar", controllerName: "Estudiante", routeValues: new {
                hash = (Session["Usuario"] as SernaSistemas.Core.Models.LoginModel).Hash,
                id = (Session["Usuario"] as SernaSistemas.Core.Models.LoginModel).IdUsuario
            }, htmlAttributes: null)
                    </li>
                    <li>
                        @Html.ActionLink(linkText: "Ver publicaciones", actionName: "ListarConceptos", controllerName: "Estudiante", routeValues: new {
                hash = (Session["Usuario"] as SernaSistemas.Core.Models.LoginModel).Hash,
                id = (Session["Usuario"] as SernaSistemas.Core.Models.LoginModel).IdUsuario
            }, htmlAttributes: null)
                    </li>
                    <li>
                        @Html.ActionLink(linkText: "Perfil", actionName: "Perfil", controllerName: "Estudiante", routeValues: new {
                hash = (Session["Usuario"] as SernaSistemas.Core.Models.LoginModel).Hash,
                id = (Session["Usuario"] as SernaSistemas.Core.Models.LoginModel).IdUsuario
            }, htmlAttributes: null)
                    </li>
                    <li>
                        @Html.ActionLink(linkText: "Acerca del proyecto", actionName: "About", controllerName: "Estudiante", routeValues: new {
                hash = (Session["Usuario"] as SernaSistemas.Core.Models.LoginModel).Hash,
                id = (Session["Usuario"] as SernaSistemas.Core.Models.LoginModel).IdUsuario
            }, htmlAttributes: null)
                    </li>
                </ul>
                <form class="navbar-form" action="@Url.Action("Buscar")" method="get">
                    <div class="input-group">
                        <input type="text" class="form-control" placeholder="Buscar" name="query" />
                        <div class="input-group-btn">
                            <button class="btn btn-warning" type="submit">
                                <i class="glyphicon glyphicon-search"></i>
                            </button>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </nav>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - Serna Sistemas</p>
        </footer>
    </div>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)

</body>
</html>
